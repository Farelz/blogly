{
    "name": "react-press",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "start": "node src/server.js --require dotenv/config",
        "pretty": "prettier-eslint --write 'src/**/*.+(css|less|js|md)'",
        "lint": "eslint --fix 'src/**/*.+(js)'",
        "format": "yarn pretty && yarn lint",
        "test": "jest"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/mocks/styleMock.js"
        },
        "setupFiles": [
            "<rootDir>/config/test-shim.js",
            "<rootDir>/config/test-setup.js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/mocks/moduleMock.js",
        "moduleFileExtensions": [
            "jsx",
            "js"
        ],
        "testMatch": [
            "**/tests/*.test.(jsx|js)"
        ]
    },
    "dependencies": {
        "@zeit/next-css": "^0.2.0",
        "@zeit/next-less": "^0.3.0",
        "apollo-boost": "^0.1.10",
        "babel-preset-env": "^1.7.0",
        "babel-preset-next": "^1.1.4",
        "babel-preset-stage-0": "^6.24.1",
        "cookie": "^0.3.1",
        "dotenv": "^5.0.1",
        "draft-js": "^0.10.5",
        "draft-js-markdown-plugin": "^2.1.0",
        "draft-js-plugins-editor": "^2.1.1",
        "draftjs-to-html": "^0.8.3",
        "express": "^4.16.3",
        "firebase": "^4.13.1",
        "graphql": "^0.13.2",
        "graphql-tag": "^2.9.2",
        "isomorphic-fetch": "^2.2.1",
        "js-cookie": "^2.2.0",
        "materialize-css": "1.0.0-beta",
        "moment": "^2.22.0",
        "next": "^6.0.3",
        "next-redux-wrapper": "^2.0.0-beta.6",
        "prop-types": "^15.6.1",
        "react": "^16.3.0",
        "react-apollo": "^2.1.9",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^16.3.0",
        "react-icons": "^2.2.7",
        "react-loadable": "^5.3.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "sanitize-html": "^1.18.2",
        "socket.io-client": "^2.1.1"
    },
    "devDependencies": {
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^8.2.3",
        "babel-jest": "^23.0.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "clean-webpack-plugin": "^0.1.19",
        "compression-webpack-plugin": "^1.1.11",
        "css-loader": "^0.28.10",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.7.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.0.6",
        "husky": "^0.14.3",
        "jest": "^23.0.0",
        "jest-dom": "^1.3.1",
        "jest-fetch-mock": "^1.6.3",
        "less": "^3.0.4",
        "less-loader": "^4.1.0",
        "lint-staged": "^7.0.4",
        "markdown": "^0.5.0",
        "path": "^0.12.7",
        "prettier": "^1.12.1",
        "prettier-eslint": "^8.8.1",
        "prettier-eslint-cli": "^4.7.1",
        "react-test-renderer": "^16.4.0",
        "react-testing-library": "^3.1.4",
        "redux-mock-store": "^1.5.1",
        "style-loader": "^0.20.3",
        "webpack-bundle-analyzer": "^2.11.1",
        "webpack-cli": "^2.0.11",
        "webpack-dev-server": "^3.1.1",
        "webpack-merge": "^4.1.2"
    }
}
