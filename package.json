{
    "name": "react-press",
    "version": "2.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "node src/server.js --require dotenv/config",
        "pretty": "prettier-eslint --write 'src/**/*.+(css|less|js|md)'",
        "lint": "eslint --fix 'src/**/*.+(js)'",
        "format": "yarn pretty && yarn lint",
        "test": "jest"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/mocks/styleMock.js"
        },
        "setupFiles": ["<rootDir>/config/test-shim.js", "<rootDir>/config/test-setup.js"],
        "setupTestFrameworkScriptFile": "<rootDir>/mocks/moduleMock.js",
        "moduleFileExtensions": ["jsx", "js"],
        "testMatch": ["**/tests/*.test.(jsx|js)"]
    },
    "dependencies": {
        "@zeit/next-css": "^0.2.0",
        "@zeit/next-less": "^0.3.0",
        "apollo-boost": "^0.1.10",
        "apollo-link-context": "^1.0.8",
        "apollo-link-http": "^1.5.4",
        "babel-preset-env": "^1.7.0",
        "babel-preset-next": "^1.1.4",
        "babel-preset-stage-0": "^6.24.1",
        "cookie": "^0.3.1",
        "date-fns": "^1.29.0",
        "dotenv": "^5.0.1",
        "draft-js": "^0.10.5",
        "draft-js-export-html": "^1.2.0",
        "draft-js-markdown-plugin": "^2.1.0",
        "draft-js-plugins-editor": "^2.1.1",
        "express": "^4.16.3",
        "firebase": "^4.13.1",
        "graphql": "^0.13.2",
        "graphql-tag": "^2.9.2",
        "isomorphic-fetch": "^2.2.1",
        "materialize-css": "1.0.0-beta",
        "moment": "^2.22.0",
        "next": "^6.0.3",
        "prop-types": "^15.6.1",
        "react": "^16.3.0",
        "react-apollo": "^2.1.9",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^16.3.0",
        "react-icons": "^2.2.7",
        "react-router-dom": "^4.2.2",
        "sanitize-html": "^1.18.2"
    },
    "devDependencies": {
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^8.2.3",
        "babel-jest": "^23.0.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.7.0",
        "jest": "^23.0.0",
        "jest-dom": "^1.3.1",
        "jest-fetch-mock": "^1.6.3",
        "less": "^3.7.1",
        "path": "^0.12.7",
        "prettier": "^1.12.1",
        "prettier-eslint": "^8.8.1",
        "prettier-eslint-cli": "^4.7.1",
        "react-test-renderer": "^16.4.0",
        "react-testing-library": "^3.1.4",
        "waait": "^1.0.2"
    }
}
